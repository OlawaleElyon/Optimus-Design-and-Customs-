{
  "version": 2,
  "installCommand": "cd frontend && yarn install && cd ../api && yarn install",
  "buildCommand": "cd frontend && yarn build",
  "outputDirectory": "frontend/build",
  "framework": null,
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
